<div class="container">
  <div class="row">
    <h3>Groups</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @groups.each do |group| %>
            <tr>
              <%= render group %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>